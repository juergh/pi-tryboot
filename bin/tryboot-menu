#!/bin/bash -eu
#
# Show tryboot entries
#

function usage()
{
	cat <<EOF
Usage: $(basename "${0}") [-h]

Show tryboot entries (TBE).

Optional arguments:
  -h, --help  Show this help text and exit.
EOF
}

while [ ${#} -ne 0 ] ; do
	case "${1}" in
		-h|--help)
			usage
			exit
			;;
		*)
			echo "Invalid argument: ${1}" >&2
			exit 2
			;;
	esac
	# shellcheck disable=SC2317
	shift
done

# shellcheck disable=SC1091
. /usr/lib/rpi-tryboot/lib.sh

tb_print_boot_menu
